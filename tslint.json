{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ false ]
    },
    "quotemark": [
        true,
        "single",
        "jsx-double"
    ],
    "semicolon": [
        true,
        "always"
    ],
    "no-console": [
        true,
        "debug",
        "info",
        "time",
        "timeEnd",
        "trace"
    ],
    "rulesDirectory": []
}